{

  "manifest_version": 2,
  "name": "Startpage",
  "version": "1.1",

  "description": "Custom Startpage for firefox",
  "icons": {
    "48": "icons/quicknote-48.png"
  },

  "permissions": [
    "storage",
    "history"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
     "32" : "icons/quicknote-32.png"
    },
    "default_title": "Startpage"
  },
"content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self'"
}
